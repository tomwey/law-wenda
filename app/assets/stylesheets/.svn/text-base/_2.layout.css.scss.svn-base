.professions {
  background:white;
  border: 1px solid #ccc;
  border-radius: 10px;
  padding: 0 10px;
}
.profession-item {
  margin: 10px 0;
  &:last-child {
    border-bottom: none;
  }
  
  h2 {
    font-size: 14px;
    margin:0;
    padding:0;
    height:30px;
    line-height: 30px;
  }
  div {
    margin-top: -5px;
  }
}

#footer {
//  border-top: 1px solid $borderColor;
  padding: $baseLineHeight 0;
}

#flash {
  margin-top: $baseLineHeight;
}

@media (max-width: 767px) {
  #footer {
    padding-left: 20px;
    padding-right: 20px;
    margin-left: -20px;
    margin-right: -20px;
  }
}

