body {
  padding-top: 8px;
  background: rgb(250,250,250);
}
#content {
	padding: 5px 0; // $baseLineHeight: 20px
  margin-bottom:  $baseLineHeight;
}

.breadcrumb {
  border: 1px solid rgb(235,235,235);
//  border-radius: 8px;
//  -webkit-border-radius: 8px;
//  -moz-border-radius: 8px;
  background: rgb(248,248,248);
}

